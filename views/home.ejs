<link rel="stylesheet" href="/css/home.css" />

<h1>Placement Cell / <%= title %></h1>

<div id="student-list-container">
  <% for(student of allStudents) { %>
  <div class="student-data-card">
    <div class="student-basic-info">
      <div class="student-heading">
        <p>Name</p>
        <p>Batch</p>
        <p>College</p>
        <p>Status</p>
        <p>DSA Score</p>
        <p>WebDev Score</p>
        <p>React Score</p>
      </div>
      <div class="student-details">
        <p><%= student.name %></p>
        <p><%= student.batch %></p>
        <p><%= student.college %></p>
        <p><%= student.status %></p>
        <p><%= student.dsa_score %></p>
        <p><%= student.webdev_score %></p>
        <p><%= student.react_score %></p>
      </div>
    </div>
    <div class="interview-details">
      <h3>Interview Info</h3>
      <div class="Interview-data">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Company</th>
              <th scope="col">Date</th>
              <th scope="col">Result</th>
            </tr>
          </thead>
          <tbody>
            <% if(allStudents.interviews){ %> <% for(interview of
            allStudents.interviews){ %>
            <tr>
              <td><%= interview.company %></td>
              <td><%= interview.date %></td>
              <td><%= interview.result %></td>
            </tr>
            <% } %> <% } %>
          </tbody>
        </table>
      </div>
    </div>
    <% } %>
  </div>

